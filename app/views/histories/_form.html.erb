<%= form_with(model: history, local: true) do |form| %>
  <% if history.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(history.errors.count, "error") %> prohibited this history from being saved:</h2>

      <ul>
        <% history.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.number_field :user_id %>
  </div>

  <div class="field">
    <%= form.label :challenge_id %>
    <%= form.number_field :challenge_id %>
  </div>

  <div class="field">
    <%= form.label :continuous_check_in %>
    <%= form.number_field :continuous_check_in %>
  </div>

  <div class="field">
    <%= form.label :finished %>
    <%= form.text_field :finished %>
  </div>

  <div class="field">
    <%= form.label :boolean %>
    <%= form.text_field :boolean %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
